import java.util.Scanner;
public class VinegreWithKey {

    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        System.out.println("input cipher");
        String inputString = scan.nextLine();
        System.out.println("input key");
        String inputKey = scan.nextLine();
        scan.close();

        char[] inputStringArr = inputString.toCharArray();
        char[] inputKeyArr = inputKey.toCharArray();

        for(int i = 0; i < inputStringArr.length; i++){ 
           inputStringArr[i] = (char) (inputStringArr[i] + inputKeyArr[i % inputKeyArr.length] - 'a');
        }
        System.out.println(new String(inputStringArr));
    }
    
}
